{
  "name": "@orbit/store",
  "version": "1.0.0",
  "description": "store module",
  "main": "./dist/index.mjs",
  "author": "Zababurin Sergey",
  "license": "GPL-3.0-only",
  "private": true,
  "config": {
    "port": 3269
  },
  "scripts": {
    "start": "start",
